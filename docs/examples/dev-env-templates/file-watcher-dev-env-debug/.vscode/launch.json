{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Node: Nodemon",
        "processId": "${command:PickProcess}",
        "restart": true,
        "protocol": "inspector"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Node: Nodemon Start",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run-script", "dev:debug"]
      }

    ]
  }